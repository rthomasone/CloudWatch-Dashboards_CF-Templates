    "Parameters": {
        "DefaultRegion": {
            "Description": "Select the default region hosting the load balancers to be monitored.",
            "Type": "String",
            "Default": "us-east-1"
        },
        "DefaultGWLB": {
            "Description": "Select the default load balancer to be monitored. (e.g. app/ALB-ABC/0123456789abcdef)",
            "Type": "String",
            "Default": ""
        }
